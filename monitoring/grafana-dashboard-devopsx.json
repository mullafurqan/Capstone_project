{
  "id": null,
  "uid": "devopsx-dashboard",
  "title": "DevOpsX 2.0 Application Dashboard",
  "tags": ["devopsx", "demo"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests Rate",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(devopsx_http_requests_total[5m])",
          "legendFormat": "{{route}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Available Replicas (devopsx-deploy)",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "kube_deployment_status_replicas_available{deployment=\"devopsx-deploy\"}",
          "legendFormat": "available replicas",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 }
    }
  ]
}

